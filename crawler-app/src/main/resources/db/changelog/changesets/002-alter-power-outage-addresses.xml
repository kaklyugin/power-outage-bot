<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <changeSet id="001-002" author="anton.kaklyugin">
        <addColumn tableName="power_outage_addresses" schemaName="public">
            <column name="line_num" type="INTEGER">
                <constraints nullable="true"/>
            </column>
        </addColumn>

        <addColumn tableName="power_outage_addresses" schemaName="public">
            <column name="parsing_status" type="VARCHAR(255)">
                <constraints nullable="true"/>
            </column>
        </addColumn>

        <addColumn tableName="power_outage_addresses" schemaName="public">
            <column name="line_html" type="TEXT">
                <constraints nullable="true"/>
            </column>
        </addColumn>
    </changeSet>

</databaseChangeLog>